<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tik.pattern">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".strategy.PatternStrategyAct"/>
        <activity android:name=".observer.PatternObserverAct"/>
        <activity android:name=".decorator.PatternDecoratorAct"/>
        <activity android:name=".factory.PatternFactoryAct"/>
        <activity android:name=".singleton.PatternSingletonAct"/>
        <activity android:name=".command.PatternCommandAct"/>
        <activity android:name=".adapter.PatternAdapterAct"/>
        <activity android:name=".facade.PatternFacadeAct"/>
        <activity android:name=".template.PatternTemplateAct"/>
        <activity android:name=".state.PatternStateAct"/>
    </application>

</manifest>